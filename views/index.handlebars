<div class = "float-left" id = "new-burgers">
  <ul>
    {{#each data}}
      {{#unless devoured}}
        <li>
          {{burger_name}}

          <button class = "devour" data-id = "{{id}}" >
            devour it </button>
        </li>
      {{/unless}}
    {{/each}}
  </ul>
</div>

<div class = "float-right" id = "eaten-burgers">
  <ul>
    {{#each data}}
      {{#if devoured}}
        <li>
          {{burger_name}}
        </li>
      {{/if}}
    {{/each}}
  <ul>
</div>
